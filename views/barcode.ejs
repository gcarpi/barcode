<!doctype html>

<html lang="en">

<head>

	<meta http-equiv="Content-Type" content="text/html; charset=utf-8">

	<title>
		<%= title %>
	</title>

	<script src="/js/webcam.min.js"></script>
	<script src="/js/app.js"></script>

	<link rel="stylesheet" href="/dist/css/style.min.css">

</head>

<body>

	<section id="barcode-container">

		<div class="barcode-content">

			<div id="barcode-camera"></div>

			<div class="barcode-square-container">
				<div class="barcode-square"></div>
			</div>

		</div>

	</section>

	<section id="barcode-result">

		<input type="text" name="result" value="" id="result">

	</section>

	<script>

		Webcam.set({

			// screen width
			width: 1024,
			height: 768,

			// device capture size
			width: 1024,
			height: 768,

			// about image
			image_format: 'jpeg',
			jpeg_quality: 90

		});

		Webcam.attach('#barcode-camera');

	</script>

	<script>
		// const supported = 'mediaDevices' in navigator;
		//
		// alert(supported);
	</script>
</body>

</html>
